<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::system">
                    <resource id="Straker_EasyTranslationPlatform::customMenu" title="Custom Menu">
                        <resource id="Straker_EasyTranslationPlatform::sampleOne" title="One" sortOrder="10" />
                        <resource id="Straker_EasyTranslationPlatform::sampleTwo" title="Two" sortOrder="10" />
                        <resource id="Straker_EasyTranslationPlatform::sampleThree" title="Three" sortOrder="10" />
                        <resource id="Straker_EasyTranslationPlatform::sampleFour" title="Four" sortOrder="10" />
                        <resource id="Straker_EasyTranslationPlatform::sampleFive" title="Five" sortOrder="10" />
                        <resource id="Straker_EasyTranslationPlatform::sampleSix" title="Five" sortOrder="10" />
                    </resource>
                </resource>

                <resource id="Magento_Backend::content">
                    <resource id="Straker_EasyTranslationPlatform::store" title="store" sortOrder="10"/>
                    <resource id="Straker_EasyTranslationPlatform::store_edit" title="store" sortOrder="10"/>
                </resource>

                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Straker_EasyTranslationPlatform::config" title="EasyTranslationPlatformConfig" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
